export REV=${REV:-/work/rlieberm/mgc/embedded}
export PATH=$REV/codebench/bin/:$PATH

export GCN_SUPPRESS_HOST_FALLBACK=1
#export GCN_DEBUG=1

export LD_LIBRARY_PATH=$REV/codebench/x86_64-none-linux-gnu/lib64:/opt/rocm/lib:$LD_LIBRARY_PATH
export LIBRARY_PATH=$LD_LIBRARY_PATH

x86_64-none-linux-gnu-gcc -O2 -fopenmp -foffload="-march=gfx90a" -ffast-math -foffload=-lm          matmul_devices.c  -o matmul_devices

export ROCR_VISIBLE_DEVICES=1,3
./matmul_devices
